{
  "name": "flask_react",
  "version": "1.0.0",
  "description": "React Frontend served by Flask",
  "main": "index.js",
  "scripts": {
    "build": "webpack -p --progress --config config/webpack.config.prod.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:stats": "webpack --env production --config config/webpack.config.prod.js --json > stats.json",
    "dev": "start python ../api/server.py && start webpack --progress -d --config config/webpack.config.dev.js --watch"
  },
  "browserslist": [
    "> .05%"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "debug": false
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "author": "Aaqib Shehzad",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "babel-loader": "^8.0.4",
    "babel-plugin-lodash": "^3.3.4",
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^5.0.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.4.4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "axios": "^0.19.0",
    "react": "^16.6.1",
    "react-dom": "^16.6.1"
  }
}
